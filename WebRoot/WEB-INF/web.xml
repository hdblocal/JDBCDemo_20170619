<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <display-name></display-name>	
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  
  <servlet>
  	<servlet-name>UserListServlet</servlet-name>
  	<servlet-class>com.suncn.servlet.UserListServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>UserListServlet</servlet-name>
  	<url-pattern>/UserListServlet</url-pattern>
  </servlet-mapping>	
	  
  <servlet>
  	<servlet-name>UserDeleteServlet</servlet-name>
  	<servlet-class>com.suncn.servlet.UserDeleteServlet</servlet-class>
  </servlet>	  
	  
  <servlet-mapping>
  	<servlet-name>UserDeleteServlet</servlet-name>
  	<url-pattern>/UserDeleteServlet</url-pattern>
  </servlet-mapping>	
  
  
  <servlet>
  	<servlet-name>UserAddServlet</servlet-name>
  	<servlet-class>com.suncn.servlet.UserAddServlet</servlet-class>
  </servlet>	  
	  
  <servlet-mapping>
  	<servlet-name>UserAddServlet</servlet-name>
  	<url-pattern>/UserAddServlet</url-pattern>
  </servlet-mapping>	  
	  
</web-app>
